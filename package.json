{
  "name": "website-test-cypress",
  "version": "1.0.0",
  "description": "Repository to tests website - https://www.automationexercise.com/",
  "main": "index.js",
  "scripts": {
    "cypress:api": "npx cypress run --spec 'cypress/e2e/API'",
    "cypress:ui": "npx cypress run --spec 'cypress/e2e/UI'",
    "chrome_headed": "npx cypress run --spec 'cypress/e2e/UI' --browser=chrome --headed",
    "chrome": "npx cypress run --spec 'cypress/e2e/UI' --browser=chrome",
    "firefox_headed": "npx cypress run --spec 'cypress/e2e/UI' --browser=firefox --headed",
    "firefox": "npx cypress run --spec 'cypress/e2e/UI' --browser=firefox",
    "edge_headed": "npx cypress run --spec 'cypress/e2e/UI' --browser=edge --headed",
    "edge": "npx cypress run --spec 'cypress/e2e/UI' --browser=edge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartoszwalasek/Website-Test-Cypress.git"
  },
  "author": "Bartosz Walasek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bartoszwalasek/Website-Test-Cypress/issues"
  },
  "homepage": "https://github.com/bartoszwalasek/Website-Test-Cypress#readme",
  "devDependencies": {
    "cypress": "^13.4.0",
    "cypress-mochawesome-reporter": "^3.7.0",
    "typescript": "^5.2.2"
  }
}
